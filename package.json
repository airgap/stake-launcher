{
  "name": "stake-launcher",
  "productName": "stake-launcher",
  "version": "0.0.1",
  "description": "Unofficial client for StakingFarm",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "prettier -c . && eslint --ext .ts,.tsx .",
    "fix": "prettier -w . && eslint --ext .ts,.tsx --fix .",
    "clean": "rm -rf .webpack node_modules dist out settings.json cookies.json",
    "postinstall": "ts-node postinstall.ts"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-wix": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-webpack": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/config": "^3.3.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "awaitx": "^0.0.7",
    "css-loader": "^6.11.0",
    "electron": "30.1.0",
    "electron-builder": "^24.13.3",
    "electron-wix-msi": "^5.1.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "from-schema": "^0.0.9",
    "node-loader": "^2.0.0",
    "puppeteer": "^22.11.2",
    "puppeteer-core": "^22.11.2",
    "react-responsive-modal": "^6.4.2",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^5.4.5",
    "use-future": "^0.0.5"
  },
  "keywords": [],
  "author": {
    "name": "Nicole Muzzin",
    "email": "nicole@muzz.in"
  },
  "license": "MIT",
  "dependencies": {
    "@cloudscape-design/components": "^3.0.662",
    "@cloudscape-design/global-styles": "^1.0.27",
    "config": "^3.3.11",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^9.0.0",
    "path-browserify": "^1.0.1",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "build": {
    "appId": "com.example.app",
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      ".webpack"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
